i2c1_build:
	cd i2c1/ ;\
	make build ;

i2c1_install:
	cd i2c1/ ;\
	make install ;

i2c1_uninstall:
	cd i2c1/ ;\
	make uninstall ;

i2c1_clean:
	cd i2c1/ ;\
	make clean;

